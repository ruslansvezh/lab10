#include <iostream>
#include <math.h>
using namespace std;
int main()
{
	setlocale(0, "");
	double N, i=1,x=1.1,y=1.1;
	cout << "Введите кол-во множителей: ";
	cin >> N;
	if (N > 0)
	{
		while (i < N)
		{
			x = x+0.1;
			y = y*x;
			i++;
		}
		cout << y;
	}
	else
		cout << "Введены неверные данные!";
	return 0;
}
